{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pytest: run all tests (with RUN_CROSSREF_INTEGRATION)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/.venv/bin/pytest",
      "args": ["-q"],
      "console": "integratedTerminal",
      "env": {
        "RUN_CROSSREF_INTEGRATION": "1"
      },
      "cwd": "${workspaceFolder}"
    }
  ]
}